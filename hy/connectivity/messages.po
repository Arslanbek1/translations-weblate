#. = the mozab driver's resource strings
#: connectivity/inc/strings.hrc:26
msgctxt "STR_ERR_EXECUTING_QUERY"
msgid "An error occurred while executing the query."
msgstr "Հարցման կատարման ժամանակ տեղի ունեցավ սխալ։"

#: connectivity/inc/strings.hrc:27
msgctxt "STR_QUERY_AT_LEAST_ONE_TABLES"
msgid "The query can not be executed. It needs at least one table."
msgstr "Հարցումը չի կարող կատարվել։ Առնվազն մեկ աղյուսակի կարիք կա։"

#: connectivity/inc/strings.hrc:28
msgctxt "STR_NO_COUNT_SUPPORT"
msgid "The driver does not support the 'COUNT' function."
msgstr "Սարքավարին չի համապատասխանում COUNT ֆունկցիան։"

#: connectivity/inc/strings.hrc:29
msgctxt "STR_STMT_TYPE_NOT_SUPPORTED"
msgid "This statement type not supported by this database driver."
msgstr "Այս օպերատորը չի համապատասխանում տվյալաշտեմարանի այս սարքավարին։"

#: connectivity/inc/strings.hrc:30
msgctxt "STR_UNSPECIFIED_ERROR"
msgid "An unknown error occurred."
msgstr "Անհայտ սխալ է տեղի ունեցել։"

#: connectivity/inc/strings.hrc:31
msgctxt "STR_ERROR_REFRESH_ROW"
msgid "An error occurred while refreshing the current row."
msgstr "Սխալ տեղի ունեցավ ընթացիկ շարքը թարմացնելու ժամանակ։"

#: connectivity/inc/strings.hrc:32
msgctxt "STR_ERROR_GET_ROW"
msgid "An error occurred while getting the current row."
msgstr "Սխալ տեղի ունեցավ ընթացիկ շարքը ստանալու ժամանակ։"

#: connectivity/inc/strings.hrc:33
msgctxt "STR_QUERY_INVALID_IS_NULL_COLUMN"
msgid "The query can not be executed. The 'IS NULL' can only be used with a column name."
msgstr "Հարցումը չի կարող գործարկվել։ IS NULL-ը կարող է գործածվել միայն սյունակի անվան հետ։"

#: connectivity/inc/strings.hrc:34
msgctxt "STR_ILLEGAL_MOVEMENT"
msgid "Illegal cursor movement occurred."
msgstr "Շարժագծի անթույլատրելի փոփոխություն։"

#: connectivity/inc/strings.hrc:35
msgctxt "STR_COMMIT_ROW"
msgid "Please commit row '$position$' before update rows or insert new rows."
msgstr "«$position$» շարքը պահպանեք նախքան թարմացումը կամ նոր շարքերի ներմուծումը։"

#. = common strings
#: connectivity/inc/strings.hrc:37
msgctxt "STR_NO_CONNECTION_GIVEN"
msgid "It doesn't exist a connection to the database."
msgstr "Տվյալաշտեմարանի հետ կապ չկա։"

#: connectivity/inc/strings.hrc:38
msgctxt "STR_WRONG_PARAM_INDEX"
msgid "You tried to set a parameter at position '$pos$' but there is/are only '$count$' parameter(s) allowed. One reason may be that the property \"ParameterNameSubstitution\" is not set to TRUE in the data source."
msgstr "Դուք փորձում էիք հարաչափ դնել «$pos$» դիրքում, բայց միայն «$count$» հարաչափն է թույլատրելի։ Մի պատճառ կարող է լինել այն, որ տվյալների աղբյուրի «ParameterNameSubstitution»  հատկությունի իբրև TRUE չի նշանակվել։"

#: connectivity/inc/strings.hrc:39
msgctxt "STR_NO_INPUTSTREAM"
msgid "The input stream was not set."
msgstr "InputStream-ը չի տրվել։"

#: connectivity/inc/strings.hrc:40
msgctxt "STR_NO_ELEMENT_NAME"
msgid "There is no element named '$name$'."
msgstr "«$name$» անունով տարր չկա։"

#: connectivity/inc/strings.hrc:41
msgctxt "STR_INVALID_BOOKMARK"
msgid "Invalid bookmark value"
msgstr "Էջանիշի սխալ արժեք։"

#: connectivity/inc/strings.hrc:42
msgctxt "STR_PRIVILEGE_NOT_GRANTED"
msgid "Privilege not granted: Only table privileges can be granted."
msgstr "Արտոնություն չի տրված։ Միայն աղյուսակի արտոնություններ կարող են տրվել։"

#: connectivity/inc/strings.hrc:43
msgctxt "STR_PRIVILEGE_NOT_REVOKED"
msgid "Privilege not revoked: Only table privileges can be revoked."
msgstr "Արտոնություն հետ չի կանչվել։ Միայն աղյուսակի արտոնություններ կարող են տրվել։"

#: connectivity/inc/strings.hrc:44
msgctxt "STR_ERRORMSG_SEQUENCE"
msgid "Function sequence error."
msgstr "Ֆունկցիայի հաջորդականության սխալ։"

#: connectivity/inc/strings.hrc:45
msgctxt "STR_INVALID_INDEX"
msgid "Invalid descriptor index."
msgstr "Նկարագրիչի սխալ ցուցակ։"

#: connectivity/inc/strings.hrc:46
msgctxt "STR_UNSUPPORTED_FUNCTION"
msgid "The driver does not support the function '$functionname$'."
msgstr "«$functionname$» ֆունկցիան սարքավարին չի համապատասխանում։ "

#: connectivity/inc/strings.hrc:47
msgctxt "STR_UNSUPPORTED_FEATURE"
msgid "The driver does not support the functionality for '$featurename$'. It is not implemented."
msgstr "«$featurename$»-ի գործառույթը չի համապատասխանում սարքավարին։ Այն չի կատարվել։"

#: connectivity/inc/strings.hrc:48
msgctxt "STR_FORMULA_WRONG"
msgid "The formula for TypeInfoSettings is wrong!"
msgstr "TypeInfoSettings-ի բանաձևը սխալ է։ "

#: connectivity/inc/strings.hrc:49
msgctxt "STR_STRING_LENGTH_EXCEEDED"
msgid "The string '$string$' exceeds the maximum length of $maxlen$ characters when converted to the target character set '$charset$'."
msgstr "«$string$» տողում կա $maxlen$-ից ավելի շատ նիշ, երբ կատարվում է թիրախային «$charset$»-ին փոխարկում։"

#: connectivity/inc/strings.hrc:50
msgctxt "STR_CANNOT_CONVERT_STRING"
msgid "The string '$string$' cannot be converted using the encoding '$charset$'."
msgstr "«$string$» տողը չի կարող փոխարկվել «$charset$» կոդավորմամբ։"

#: connectivity/inc/strings.hrc:51
msgctxt "STR_URI_SYNTAX_ERROR"
msgid "The connection URL is invalid."
msgstr "Միացման URL-ը սխալ է։"

#: connectivity/inc/strings.hrc:52
msgctxt "STR_QUERY_TOO_COMPLEX"
msgid "The query can not be executed. It is too complex."
msgstr "Հարցումը չի կարող կատարվել, շատ բարդ է։"

#: connectivity/inc/strings.hrc:53
msgctxt "STR_OPERATOR_TOO_COMPLEX"
msgid "The query can not be executed. The operator is too complex."
msgstr "Հարցումը չի կարող կատարվել։ Օպերատորը շատ բարդ է։"

#: connectivity/inc/strings.hrc:54
msgctxt "STR_QUERY_INVALID_LIKE_COLUMN"
msgid "The query can not be executed. You cannot use 'LIKE' with columns of this type."
msgstr "Հարցումը չի կարող կատարվել։ Չեք կարող գործածել «LIKE»-ն այս տեսակի սյունակների հետ։"

#: connectivity/inc/strings.hrc:55
msgctxt "STR_QUERY_INVALID_LIKE_STRING"
msgid "The query can not be executed. 'LIKE' can be used with a string argument only."
msgstr "Հարցումը չի կարող կատարվել։ «LIKE»-ը կարող է գործածվել միայն տողային արգումենտի հետ։"

#: connectivity/inc/strings.hrc:56
msgctxt "STR_QUERY_NOT_LIKE_TOO_COMPLEX"
msgid "The query can not be executed. The 'NOT LIKE' condition is too complex."
msgstr "Հարցումը չի կարող կատարվել։ «NOT LIKE» պայմանը շատ բարդ է։"

#: connectivity/inc/strings.hrc:57
msgctxt "STR_QUERY_LIKE_WILDCARD"
msgid "The query can not be executed. The 'LIKE' condition contains wildcard in the middle."
msgstr "Հարցումը չի կարող կատարվել։ «LIKE» պայմանի դերանշանը մեջտեղում է։"

#: connectivity/inc/strings.hrc:58
msgctxt "STR_QUERY_LIKE_WILDCARD_MANY"
msgid "The query can not be executed. The 'LIKE' condition contains too many wildcards."
msgstr "Հարցումը չի կարող կատարվել։ «LIKE» պայմանում դերանշանները չափազանց շատ են։"

#: connectivity/inc/strings.hrc:59
msgctxt "STR_INVALID_COLUMNNAME"
msgid "The column name '$columnname$' is not valid."
msgstr "Սյունակի «$columnname$» անունը թույլատրելի չէ։"

#: connectivity/inc/strings.hrc:60
msgctxt "STR_INVALID_COLUMN_SELECTION"
msgid "The statement contains an invalid selection of columns."
msgstr "Օպերատորի սյունակները սխալ են ընտրված։"

#: connectivity/inc/strings.hrc:61
msgctxt "STR_COLUMN_NOT_UPDATEABLE"
msgid "The column at position '$position$' could not be updated."
msgstr "«$position$» դիրքի սյունակը չի կարող թարմացվել։"

#: connectivity/inc/strings.hrc:62
msgctxt "STR_COULD_NOT_LOAD_FILE"
msgid "The file $filename$ could not be loaded."
msgstr "$filename$ նիշքը չի կարող բեռնվել։"

#: connectivity/inc/strings.hrc:63
msgctxt "STR_LOAD_FILE_ERROR_MESSAGE"
msgid ""
"The attempt to load the file resulted in the following error message ($exception_type$):\n"
"\n"
"$error_message$"
msgstr ""
"Նիշքի բեռնման փորձը ձախողվեց հետևյալ սխալի պատճառով՝ ($exception_type$)։\n"
"\n"
"$error_message$"

#. = the ado driver's resource strings
#: connectivity/inc/strings.hrc:65
msgctxt "STR_TYPE_NOT_CONVERT"
msgid "The type could not be converted."
msgstr "Այս տեսակը չի կարող փոխարկվել։"

#: connectivity/inc/strings.hrc:66
msgctxt "STR_INVALID_COLUMN_DESCRIPTOR_ERROR"
msgid "Could not append column: invalid column descriptor."
msgstr "Սյունակը հնարավոր չէ ավելացնել սխալ նկարագրիչի պատճառով։"

#: connectivity/inc/strings.hrc:67
msgctxt "STR_INVALID_GROUP_DESCRIPTOR_ERROR"
msgid "Could not create group: invalid object descriptor."
msgstr "Խումբը չի ստեղծվել. տարրի սխալ նկարագրիչ։"

#: connectivity/inc/strings.hrc:68
msgctxt "STR_INVALID_INDEX_DESCRIPTOR_ERROR"
msgid "Could not create index: invalid object descriptor."
msgstr "Ցուցակը չի ստեղծվել. տարրի սխալ նկարագրիչ։"

#: connectivity/inc/strings.hrc:69
msgctxt "STR_INVALID_KEY_DESCRIPTOR_ERROR"
msgid "Could not create key: invalid object descriptor."
msgstr "Բանալին չի ստեղծվել. տարրի սխալ նկարագրիչ։"

#: connectivity/inc/strings.hrc:70
msgctxt "STR_INVALID_TABLE_DESCRIPTOR_ERROR"
msgid "Could not create table: invalid object descriptor."
msgstr "Աղյուսակը չի ստեղծվել. տարրի սխալ նկարագրիչ։"

#: connectivity/inc/strings.hrc:71
msgctxt "STR_INVALID_USER_DESCRIPTOR_ERROR"
msgid "Could not create user: invalid object descriptor."
msgstr "Օգտատերը չի ստեղծվել. տարրի սխալ նկարագրիչ։"

#: connectivity/inc/strings.hrc:72
msgctxt "STR_INVALID_VIEW_DESCRIPTOR_ERROR"
msgid "Could not create view: invalid object descriptor."
msgstr "Տեսքը չի ստեղծվել. տարրի սխալ նկարագրիչ։"

#: connectivity/inc/strings.hrc:73
msgctxt "STR_VIEW_NO_COMMAND_ERROR"
msgid "Could not create view: no command object."
msgstr "Տեսքը չի ստեղծվել. Command տարր չկա։"

#: connectivity/inc/strings.hrc:74
msgctxt "STR_NO_CONNECTION"
msgid "The connection could not be created. May be the necessary data provider is not installed."
msgstr "Միացումը չի կատարվել։ Թերևս չի տեղակայված անհրաժեշտ տվյալների աղբյուրը։"

#: connectivity/inc/strings.hrc:76
msgctxt "STR_COULD_NOT_DELETE_INDEX"
msgid "The index could not be deleted. An unknown error while accessing the file system occurred."
msgstr "Ցուցակը չի ջնջվել։ Անհայտ սխալ տեղի ունեցավ նիշքային համակարգին մատչելու ժամանակ։"

#: connectivity/inc/strings.hrc:77
msgctxt "STR_ONL_ONE_COLUMN_PER_INDEX"
msgid "The index could not be created. Only one column per index is allowed."
msgstr "Ցուցակը չի ստեղծվել։ Մեկ ցուցակը միայն մեկ սյունակ կարող է ունենալ։"

#: connectivity/inc/strings.hrc:78
msgctxt "STR_COULD_NOT_CREATE_INDEX_NOT_UNIQUE"
msgid "The index could not be created. The values are not unique."
msgstr "Ցուցակը չի ստեղծվել։ Արժեքներն ինքնատիպ չեն։"

#: connectivity/inc/strings.hrc:79
msgctxt "STR_COULD_NOT_CREATE_INDEX"
msgid "The index could not be created. An unknown error appeared."
msgstr "Ցուցակը չի ստեղծվել։ Անհայտ սխալ տեղի ունեցավ։"

#: connectivity/inc/strings.hrc:80
msgctxt "STR_COULD_NOT_CREATE_INDEX_NAME"
msgid "The index could not be created. The file '$filename$' is used by another index."
msgstr "Ցուցակը չի ստեղծվել։ «$filename$»  նիշքը գործածվում է այլ ցուցակի կողմից։"

#: connectivity/inc/strings.hrc:81
msgctxt "STR_COULD_NOT_CREATE_INDEX_KEYSIZE"
msgid "The index could not be created. The size of the chosen column is too big."
msgstr "Ցուցակը չի ստեղծվել։ Ընտրված սյունակի չափերը շատ մեծ են։"

#: connectivity/inc/strings.hrc:82
msgctxt "STR_SQL_NAME_ERROR"
msgid "The name '$name$' doesn't match SQL naming constraints."
msgstr "«$name$» անունը չի համապատասխանում SQL-ի անվանային սահմանափակումներին։ "

#: connectivity/inc/strings.hrc:83
msgctxt "STR_COULD_NOT_DELETE_FILE"
msgid "The file $filename$ could not be deleted."
msgstr "$filename$ նիշքը չի ջնջվել։"

#: connectivity/inc/strings.hrc:84
msgctxt "STR_INVALID_COLUMN_TYPE"
msgid "Invalid column type for column '$columnname$'."
msgstr "«$columnname$»-ի սյունակի սխալ տեսակ։"

#: connectivity/inc/strings.hrc:85
msgctxt "STR_INVALID_COLUMN_PRECISION"
msgid "Invalid precision for column '$columnname$'."
msgstr "«$columnname$» սյունակի թվանշանների սխալ քանակ։"

#: connectivity/inc/strings.hrc:86
msgctxt "STR_INVALID_PRECISION_SCALE"
msgid "Precision is less than scale for column '$columnname$'."
msgstr "«$columnname$» սյունակի թվանշաններն ավելի քիչ են, քան տասնորդականի թվանշանները։ "

#: connectivity/inc/strings.hrc:87
msgctxt "STR_INVALID_COLUMN_NAME_LENGTH"
msgid "Invalid column name length for column '$columnname$'."
msgstr "«$columnname$» սյունակի անվան սխալ երկարություն։"

#: connectivity/inc/strings.hrc:88
msgctxt "STR_DUPLICATE_VALUE_IN_COLUMN"
msgid "Duplicate value found in column '$columnname$'."
msgstr "«$columnname$» սյունակում կա կրկնված արժեք։"

#: connectivity/inc/strings.hrc:89
msgctxt "STR_INVALID_COLUMN_DECIMAL_VALUE"
msgid ""
"The '$columnname$' column has been defined as a \"Decimal\" type, the max. length is $precision$ characters (with $scale$ decimal places).\n"
"\n"
"The specified value \"$value$ is longer than the number of digits allowed."
msgstr ""
"«$columnname$» աղյուսակը «Decimal» տիպի է, առավելագույն երկարությունը՝ $precision$ նիշ ($scale$ տասնորդական թվանշանների հետ)։\n"
"\n"
"\"$value$ արժեքն ավելի երկար է թվանշանների թույլատրելի քանակից։ "

#: connectivity/inc/strings.hrc:90
msgctxt "STR_COLUMN_NOT_ALTERABLE"
msgid "The column '$columnname$' could not be altered. May be the file system is write protected."
msgstr "«$columnname$» սյունակը չի կարող փոփոխվել։ Թերևս նիշքային համակարգը գրանցումից պաշտպանված է։"

#: connectivity/inc/strings.hrc:91
msgctxt "STR_INVALID_COLUMN_VALUE"
msgid "The column '$columnname$' could not be updated. The value is invalid for that column."
msgstr "«$columnname$» սյունակը չի կարող թարմացվել։ Այդ սյունակի արժեքը սխալ է։"

#: connectivity/inc/strings.hrc:92
msgctxt "STR_COLUMN_NOT_ADDABLE"
msgid "The column '$columnname$' could not be added. May be the file system is write protected."
msgstr "«$columnname$» սյունակը չի կարող ավելացվել։ Թերևս նիշքային համակարգը պաշտպանված է գրանցումից։"

#: connectivity/inc/strings.hrc:93
msgctxt "STR_COLUMN_NOT_DROP"
msgid "The column at position '$position$' could not be dropped. May be the file system is write protected."
msgstr "«$position$» դիրքի սյունակը չի կարող հեռացվել։ Թերևս նիշքային համակարգը գրանցումից պաշտպանված է։ "

#: connectivity/inc/strings.hrc:94
msgctxt "STR_TABLE_NOT_DROP"
msgid "The table '$tablename$' could not be dropped. May be the file system is write protected."
msgstr "«$tablename$» աղյուսակը չի կարող հեռացվել։ Թերևս նիշքային համակարգը գրանցումից պաշտպանված է։"

#: connectivity/inc/strings.hrc:95
msgctxt "STR_COULD_NOT_ALTER_TABLE"
msgid "The table could not be altered."
msgstr "Աղյուսակը չի կարող փոփոխվել։"

#: connectivity/inc/strings.hrc:96
msgctxt "STR_INVALID_DBASE_FILE"
msgid "The file '$filename$' is an invalid (or unrecognized) dBase file."
msgstr "«$filename$»-ը սխալ (կամ անհայտ) dBase նիշք է։"

#. Evoab2
#: connectivity/inc/strings.hrc:98
msgctxt "STR_CANNOT_OPEN_BOOK"
msgid "Cannot open Evolution address book."
msgstr "Evolution-ի հասցեագիրքը չի հաջողվում բացել։"

#: connectivity/inc/strings.hrc:99
msgctxt "STR_SORT_BY_COL_ONLY"
msgid "Can only sort by table columns."
msgstr "Հնարավոր է տեսակավորել միայն աղյուսակի սյունակներով։"

#: connectivity/inc/strings.hrc:101
msgctxt "STR_QUERY_COMPLEX_COUNT"
msgid "The query can not be executed. It is too complex. Only \"COUNT(*)\" is supported."
msgstr "Հարցումը չի կարող կատարվել։ Այն խիստ բարդ է։ Հնարավոր է միայն «COUNT(*)»-ը։"

#: connectivity/inc/strings.hrc:102
msgctxt "STR_QUERY_INVALID_BETWEEN"
msgid "The query can not be executed. The 'BETWEEN' arguments are not correct."
msgstr "Հարցումը չի կարող կատարվել։ «BETWEEN»-ի արգումենտները սխալ են։"

#: connectivity/inc/strings.hrc:103
msgctxt "STR_QUERY_FUNCTION_NOT_SUPPORTED"
msgid "The query can not be executed. The function is not supported."
msgstr "Հարցումը չի կարող կատարվել։ Ֆունկցիան չկա։"

#: connectivity/inc/strings.hrc:104
msgctxt "STR_TABLE_READONLY"
msgid "The table can not be changed. It is read only."
msgstr "Աղյուսակը չի կարող փոփոխվել։ Միայն ընթերցման համար է։"

#: connectivity/inc/strings.hrc:105
msgctxt "STR_DELETE_ROW"
msgid "The row could not be deleted. The option \"Display inactive records\" is set."
msgstr "Տողը չի կարող ջնջվել։ Միացված է «Ցույց տալ ոչ ակտիվ գրառումները» ընտրանքը։"

#: connectivity/inc/strings.hrc:106
msgctxt "STR_ROW_ALREADY_DELETED"
msgid "The row could not be deleted. It is already deleted."
msgstr "Տողը չի կարող ջնջվել։ Այն արդեն ջնջված է։"

#: connectivity/inc/strings.hrc:107
msgctxt "STR_QUERY_MORE_TABLES"
msgid "The query can not be executed. It contains more than one table."
msgstr "Հարցումը չի կարող կատարվել։ Պարունակում է մեկից ավելի աղյուսակ։"

#: connectivity/inc/strings.hrc:108
msgctxt "STR_QUERY_NO_TABLE"
msgid "The query can not be executed. It contains no valid table."
msgstr "Հարցումը չի կարող կատարվել։ Չի պարունակում թույլատրելի աղյուսակ։"

#: connectivity/inc/strings.hrc:109
msgctxt "STR_QUERY_NO_COLUMN"
msgid "The query can not be executed. It contains no valid columns."
msgstr "Հարցումը չի կարող կատարվել։ Չի պարունակում թույլատրելի սյունակներ։"

#: connectivity/inc/strings.hrc:110
msgctxt "STR_INVALID_PARA_COUNT"
msgid "The count of the given parameter values doesn't match the parameters."
msgstr "Հարաչափի արժեքների քանակը չի համապատասխանում հարաչափերին։"

#: connectivity/inc/strings.hrc:111
msgctxt "STR_NO_VALID_FILE_URL"
msgid "The URL '$URL$' is not valid. A connection can not be created."
msgstr "'$URL$' URL-ը սխալ է։ Միացում չի կարող կատարվել։"

#: connectivity/inc/strings.hrc:112
msgctxt "STR_NO_CLASSNAME"
msgid "The driver class '$classname$' could not be loaded."
msgstr "Սարքավարի «$classname$» տեսակը չի կարող բեռնվել։ "

#: connectivity/inc/strings.hrc:113
msgctxt "STR_NO_JAVA"
msgid "No Java installation could be found. Please check your installation."
msgstr "Չգտնվեց տեղակայված Java։ Ստուգեք տեղակայումը։  "

#: connectivity/inc/strings.hrc:114
msgctxt "STR_NO_RESULTSET"
msgid "The execution of the query doesn't return a valid result set."
msgstr "Հարցման գործարկումը չի բերում թույլատրելի արդյունքներ։"

#: connectivity/inc/strings.hrc:115
msgctxt "STR_NO_ROWCOUNT"
msgid "The execution of the update statement doesn't effect any rows."
msgstr "Թարմացման հարցման կատարումը չփոխեց որևէ տող։"

#: connectivity/inc/strings.hrc:116
msgctxt "STR_NO_CLASSNAME_PATH"
msgid "The additional driver class path is '$classpath$'."
msgstr "Սարքավարի տեսակի հավելյալ ուղին «$classpath$»-ն է։"

