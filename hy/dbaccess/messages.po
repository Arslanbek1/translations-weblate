#: dbaccess/inc/strings.hrc:459
msgctxt "STR_EXCEPTION_WARNING"
msgid "Warning"
msgstr "Զգուշացում"

#. LSBpE
#: dbaccess/inc/strings.hrc:460
msgctxt "STR_EXCEPTION_INFO"
msgid "Information"
msgstr "Տեղեկույթ"

#. DKRwR
#: dbaccess/inc/strings.hrc:461
